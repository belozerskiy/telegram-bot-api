{
  "name": "backend-boilerplete",
  "version": "1.0.0",
  "main": "index.js",
  "license": "proprietary",
  "scripts": {
    "start": "babel -d ./dist ./src -s",
    "dev": "nodemon dev.js",
    "lint:js": "eslint ./src",
    "doc:js": "jsdoc -d ./doc ./src",
    "test": "",
    "precommit": "lint-staged && yarn run doc:js"
  },
  "lint-staged": {
    "*.js": ["eslint"]
  },
  "pre-commit": "precommit",
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.14.9",
    "pre-commit": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": ["airbnb-base", "prettier"],
    "env": {
      "node": true
    }
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "lodash": "^4.17.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
